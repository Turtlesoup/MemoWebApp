<div class="centerPanel">
  <%= form_tag password_resets_path, :method => :post do %>
    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
    </div>
    <br>
    <div><%= submit_tag "Reset Password", :class => 'greenButton' %></div>
  <% end %>
</div>
